{
    "version": "0.2",
    "language": "en",
    "analyze_file_names": "false",

    "words": [
        "RSAT"
    ],

    "ignoreWords": [
        "stefanzweifel",
        "diffy"
    ],
    
    "flagWords": [
        "yeet"
    ],

    "patterns": [
        {
          "name": "ALL-CAPS-WORDS",
          "pattern": "/\b[A-Z0-9]+\b/g",
          "description": "Any word in ALL CAPS."
        }
    ],

    "ignoreRegExpList": [
        "ALL-CAPS-WORDS",
        "Email",
        "github.com/",
        "@"
    ]
}
